.page1 .tit { position: absolute; top: 0; left: 0; width: 100%; height: 100%; background: url(../images/page1/title.png) no-repeat 0 0; background-size: 100%; text-indent: -999em; z-index: 3; pointer-events: none; }
.page1 .car { position: absolute; bottom: 0; left: 0; width: 640px; height: 546px; background: url(../images/page1/car.png) no-repeat 0 0;background-size: contain; }
