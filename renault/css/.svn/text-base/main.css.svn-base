html, body { height: 100%; }
/*reset*/
ol{list-style:none;}
ol li,ol{padding:0px;margin:0px;}
/*common*/

.hide{display: none;}
.ui-btn{border:3px solid #ffa800;border-radius:10px;font-size:32px;font-weight:900;text-align:center;padding:13px 0px;display:block;color:#3d3c3c;text-decoration: initial;}
.clearfix:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
body{display: none;}

.page1 { position: relative; width: 640px; height: 100%; background: url(../images/page1/bg.jpg) no-repeat 0 0; background-size: 100% 100%; overflow: hidden; }
.page1 .snow{ position: absolute; top: 0; left: 0;width: 100%;height: 100%;background: url(../images/page1/snow.png); background-size: 100%;z-index:2;pointer-events: none; background-position: 0 -400px; background-repeat:repeat-y; }
