.page1 .qq2 { position: absolute; bottom: 528px; right: 10px; width: 124px; height: 195px; background: url(../images/page1/qq2.png) no-repeat 0 0; background-size: contain; }
.page1 .txt { position: absolute; bottom: -20px; left: 50%; width: 503px; margin-left: -227px; height: 361px; background: url(../images/page1/txt.png) no-repeat 0 0; background-size: contain; }
.page1 #luckBtn { position: absolute; width: 450px; height: 82px; left: 0; top: 45px; text-indent: -999em; }
.page1 #manualBtn { position: absolute; width: 196px; height: 68px; bottom: 0px; left: 10px; text-indent: -999em;}
.page1 #prizeBtn { position: absolute; width: 196px; height: 68px; bottom: 0px; left: 240px; text-indent: -999em;}

